<!DOCTYPE html>
<html>
<head>
  <title>Jekyll Integration app</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <% if Rails.env.production? %>
    <base href="/ror/sso_client/" />
  <% end %>
  <%= yield :cdn_code %>
  <%= javascript_include_tag '//cdn.auth0.com/js/lock/10.0.0/lock.min.js' %>
  <%= stylesheet_link_tag params[:controller] %>
  <%= stylesheet_link_tag    'manager_layout', media: 'all'%>
  <%= javascript_include_tag 'manager_layout' %>
  <%= csrf_meta_tags %>
</head>
<body class="inner-bg">
  <div id="hand-over"></div>
  <%= render  'layouts/header' %>
  <div class="page-wapper give-height o-wrapper" id="o-wrapper">
    <div id="top-banner">
      <div class="inner-banner">
        <div class="up-info">
          <div class="container">
            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-12">
                Administratorâ€™s Documentation
              </div>
            </div>
          </div>
        </div>
        <%= image_tag "administrator_documentation.jpg" %>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12">
          <div class="left-panel">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
  <%= render 'layouts/footer' %>

  </div>
  <%= render 'layouts/sidebar' %>
  <div id="c-mask" class="c-mask"></div>
</body>
</html>
